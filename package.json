  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:dev": "yarn postgres:dev && yarn redis:dev",
    "db:dev:kill": "yarn postgres:dev:kill && redis:dev:kill",
    "redis:dev": "sudo docker run -dit --rm --name=fancy-jackal -v $PWD/redis-dev.conf:/usr/local/etc/redis/redis.conf -p 6379:6379 redis:5.0.8",
    "redis:dev:kill": "sudo docker kill fancy-jackal",
    "redis-cli:dev": "sudo docker exec -it fancy-jackal redis-cli",
    "postgres:dev": "sudo docker run --name fancy-parrot -e POSTGRES_PASSWORD=schism -p 5432:5432 -d --rm postgres:13.3",
    "postgres:dev:kill": "sudo docker kill fancy-parrot",
    "psql:dev": "sudo docker exec -it -u postgres fancy-parrot psql",
    "prisma:studio:dev": "dotenv -e .env.development.local -- npx prisma studio",
    "prisma:studio:prod": "dotenv -e .env.production.local -- npx prisma studio",
    "prisma:db:push:dev": "dotenv -e .env.development.local -- npx prisma db push",
    "prisma:generate:client:dev": "dotenv -e .env.development.local -- npx prisma generate",
    "prisma:migrate:init:dev": "dotenv -e .env.development.local -- npx prisma migrate dev --name init",
    "prisma:migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma:db:push:prod": "dotenv -e .env.production.local -- npx prisma db push",
    "prisma:generate:client:prod": "dotenv -e .env.production.local -- npx prisma generate",
    "prisma:migrate:init:prod": "dotenv -e .env.production.local -- npx prisma migrate dev --name init",
    "prisma:migrate:prod": "dotenv -e .env.production.local -- npx prisma migrate dev",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "lint": "next lint",
    "prisma:introspect": "dotenv -e .env.production.local -- npx prisma introspect",
    "prisma:seed": "dotenv -e .env.production.local -- npm run ts-node prisma/seed.ts --preview-feature"
  },
